<view>
  <view class='avatar-view'>
    <open-data type="userAvatarUrl" class='avatar'></open-data>
  </view>
  <view class='nickname'>
    <open-data type="userNickName"></open-data>
  </view>
  <view class='feedback' bindtap='goFeedback'>
    <view class='feedback-left'>
      <view>
        <image src='../../images/feedback.png' class='feedback-icon'></image>
      </view>
      <view>意见反馈</view>
    </view>
    <view>
      <image src='../../images/arrowRight.png' class='arrowRight-icon'></image>
    </view>
  </view>
  <view class='about-us' bindtap='goAboutUs'>
    <view class='about-us-left'>
      <view>
        <image src='../../images/about.png' class='about-us-icon'></image>
      </view>
      <view>关于我们</view>
    </view>
    <view>
      <image src='../../images/arrowRight.png' class='arrowRight-icon'></image>
    </view>
  </view>
</view>