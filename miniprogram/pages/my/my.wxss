.avatar-view {
  margin: 100rpx 0 40rpx;
  display: flex;
  justify-content: center;
}

.avatar {
  width: 200rpx;
  height: 200rpx;
}

.nickname {
  text-align: center;
}

.feedback-icon, .about-us-icon {
  width: 40rpx;
  height: 40rpx;
  display: block;
  margin-right: 40rpx;
}

.arrowRight-icon {
  width: 40rpx;
  height: 40rpx;
  display: block;
}

.feedback {
  display: flex;
  align-items: center;
  background-color: #f6f6f6;
  line-height: 100rpx;
  justify-content: space-between;
  padding: 0 20rpx;
  margin: 80rpx 0;
}

.about-us {
  display: flex;
  align-items: center;
  background-color: #f6f6f6;
  line-height: 100rpx;
  justify-content: space-between;
  padding: 0 20rpx;
}

.feedback-left, .about-us-left {
  display: flex;
  align-items: center;
}
